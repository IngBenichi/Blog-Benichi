{
    "builds": [
        {
            "src": "mysite/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.13.1",
                "postInstallCommand": "python manage.py collectstatic --noinput"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        }
    ]
}